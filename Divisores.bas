10 REM DIVISORES
20 CLS
22 GOTO 30
25 PRINT
30 INPUT "N="; N
35 PRINT "DIVISORES:";
40 FOR DVS = 1 TO N - 1: REM EL "-1" ES ADICIONAL
45 C = INT(N / DVS)
50 R = N - C * DVS
70 IF R = 0 THEN PRINT ; DVS; : IF R = 0 THEN LET ND = ND + 1
100 NEXT DVS
105 PRINT N: REM LINEA NECESARIA UNICAMENTE SI 40="FOR DVS = 1 TO N - 1"
108 IF ND <= 1 THEN PRINT "NUMERO PRIMO"
110 INPUT "`C` PARA CONTINUAR, `E` PARA SALIR.  ", A$
120 IF A$ = "E" THEN GOTO 150
130 IF A$ = "C" THEN GOTO 25
140 GOTO 110
150 END

